void setup(){
size (500,500);

}
void draw(){
if(keyPressed==true)
if(key=='1'){
 line(100,0,height,width);
} else if(key=='2'){
line(200,10,250,250);
}else if(key=='3'){
  line(0,100,height,width);
}else if (key=='4'){
  stroke(255);
}else if (key=='5'){
  background(255,204,0);
}
}
